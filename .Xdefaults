! URxvt Settings

! decos
*scrollBar:         false
*internalBorder:    10
*cursorBlink:       true
*.font:             xft:Fira Code:size=12

! Police settings for X11
Xft*antialias: true
Xft*hinting: true
Xft*hintstyle: hintfull
Xft*rgba: rgb

! Clickable URLs
URxvt.perl-ext-common: default,matcher,resize-font
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
! resize-font
URxvt.keysym.C-k:   font-size:increase
URxvt.keysym.C-j:   font-size:decrease

! Colorscheme https://terminal.sexy

! tango

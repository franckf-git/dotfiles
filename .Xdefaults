! URxvt Settings
! decos
*scrollBar:         false
*internalBorder:    10
*cursorBlink:       true
*.font:             xft:JetBrains Mono:size=16

! Police settings for X11
Xft*antialias: true
Xft*hinting: true
Xft*hintstyle: hintfull
Xft*rgba: rgb

! Clickable URLs
URxvt.perl-ext-common: default,matcher,resize-font
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
! resize-font
URxvt.keysym.C-k:   font-size:increase
URxvt.keysym.C-j:   font-size:decrease

! Colorscheme https://terminal.sexy
! special
*.foreground:   #e9e9e9
*.background:   #1f1f1f
*.cursorColor:  #e9e9e9

! black
*.color0:       #2a1d17
*.color8:       #4f362b

! red
*.color1:       #da1657
*.color9:       #da4375

! green
*.color2:       #3ea250
*.color10:      #6cb87a

! yellow
*.color3:       #e3d33d
*.color11:      #e3da84

! blue
*.color4:       #3ea290
*.color12:      #04b091

! magenta
*.color5:       #ff850d
*.color13:      #ffa64f

! cyan
*.color6:       #8c16da
*.color14:      #a167c7

! white
*.color7:       #e9e9e9
*.color15:      #fdfdfd

general {
        colors = false
        interval = 5
}

order += "wireless wlp0s29u1u5"
order += "tztime date"
order += "tztime hour"

wireless wlp0s29u1u5 {
        format_up = "  wifi %quality  "
        format_down = "  wifi down  "
}

battery 0 {
        format = "  %status %percentage  "
        format_down = "No battery"
        status_chr = "CHR"
        status_bat = "BAT"
        status_unk = "UNK"
        status_full = "FULL"
        path = "/sys/class/power_supply/BAT%d/uevent"
        low_threshold = 10
}

tztime date {
        format = "  %a %d/%m  "
}

tztime hour {
        format = "  %H:%M  "
}
